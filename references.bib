@article{John:2004,
author = {Hsu John C. and Butterfield Marion},
title = {Modeling Emergent Behavior for Systems‐of‐Systems},
journal = {INCOSE International Symposium},
volume = {17},
number = {1},
pages = {1811-1821},
doi = {10.1002/j.2334-5837.2007.tb02985.x},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/j.2334-5837.2007.tb02985.x},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/j.2334-5837.2007.tb02985.x},
abstract = {Abstract Emergent behaviors exist in biological systems, physical systems and human performance. Little is currently known about constructing an interoperable network of systems and the incorporation of known emergent behaviors. Definitions of emergence are introduced. The four principles of emergence are outlined and discussed. Conceptual agent‐based modeling is discussed to illustrate possible modeling approaches for identifying and assessing emergent behaviors according to the emergent principles. The agent‐based simulation will be the inputs to system‐of‐systems (SoS) architectural models. Neural network artificial intelligence may be needed to assist our understanding of the emergent behaviors during operations of SoS and the associated system architecture model development. It is recommended that the integration of agent‐based simulation and neural network methods with SysML be considered.}
}

